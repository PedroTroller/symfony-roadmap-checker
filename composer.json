{
    "name": "pedrotroller/symfony-roadmap-checker",
    "description": "",
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "symfony/config": ">=2.0",
        "symfony/dependency-injection": ">=2.0",
        "symfony/event-dispatcher": ">=2.0",
        "symfony/filesystem": ">=2.0",
        "symfony/http-kernel": ">=2.0"
    },
    "require-dev": {
        "pedrotroller/php-cs-custom-fixer": "~1.6.0",
        "phpspec/phpspec": "~4.0.0",
        "phpstan/phpstan": "~0.8.3"
    },
    "autoload": {
        "psr-4": {
            "PedroTroller\\SymfonyRoadmapChecker\\": "src/PedroTroller/SymfonyRoadmapChecker/"
        }
    },
    "config": {
        "sort-packages": true
